@model PR_Top_Service_MVC.Models.PersonAdmin

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Estas seguro de eliminar este registro?</h3>
<div>
    <h4>PersonAdmin</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @ViewBag.person.Name
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.person.LastName
        </dd>
        <dt class = "col-sm-2">
            @ViewBag.user.Email
        </dt>
        
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdAdmin" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
